{
    "extends": "astro/tsconfigs/strictest",
    "compilerOptions": {
        "strictNullChecks": true,
        "noUncheckedIndexedAccess": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "outDir": "dist/tsc",
    },
    "include": [
        ".astro/types.d.ts",
        "**/*"
    ],
    "exclude": [
        "dist",
        "**/*.snap",
        "node_modules"
    ],
}
